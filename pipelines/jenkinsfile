node{
    checkout scm
    env.PYTHON_ALIAS = 'python3'
    env.SOURCECODE_FOLDER = 'scm'
    activity = load "$SOURCECODE_FOLDER/pipelines/templates/jenkins/activity.groovy"
    activity.loadEnvVariables()

    activity.addStage('start')()
    activity.addParallelStages('Iterations',
        [
            'AHRS_Voter' : { 
                activity.addStage('AHRS_Voter_CollectMetrics')()
            },
            'Actuator_Control' : {
                activity.addStage('Actuator_Control_CollectMetrics')()
            },
            'Flight_Control' : { 
                activity.addStage('Flight_Control_CollectMetrics')()
            },
            'InnerLoop_Control' : { 
                activity.addStage('InnerLoop_Control_CollectMetrics')()
            },
            'OuterLoop_Control' : { 
                activity.addStage('OuterLoop_Control_CollectMetrics')()
            },

        ]
    )()
    activity.addStage('end')()
}


    